{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "requireConfigFile": false,
        "sourceType": "module",
        "parser": "@babel/eslint-parser"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": ["off"]
    },
    "globals": {
        "$": true,
        "require": true,
        "process": true
    }
}
